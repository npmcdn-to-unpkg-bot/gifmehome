-# = render 'dropzone'

= form_for photo, url: admin_photos_path, html: { multipart: true, class: 'form-horizontal' } do |form|
  .form-group
    %label.col-sm-2.control-label(for='photo_image')
      Plik z zdjęciem
    .col-sm-10
      = form.file_field :image
  .form-group
    %label.col-sm-2.control-label(for='photo_name')
      Imię zwierzaka
    .col-sm-10
      = form.text_field :name, class: 'form-control'
  .form-group
    .col-sm-offset-2.col-sm-10
      %button(type="submit" class="btn btn-default")
        Prześlij